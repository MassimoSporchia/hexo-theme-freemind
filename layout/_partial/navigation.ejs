<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#mainNavbar" aria-controls="mainNavbar"
    aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <a class="navbar-brand" href="<%- config.root %>">
    <%= config.title %>
  </a>
  <div class="collapse navbar-collapse" id="mainNavbar">
    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
      <% theme.menu.forEach(function(m){ %>
        <li class="nav-item active">
          <a class="nav-link" href="<%- config.root %><%- m.url %>" title="<%- m.intro %>">
            <%- m.title %>
          </a>
        </li>
        <% }) %>
    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="search" placeholder="Search">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
    </form>
  </div>
</nav>
